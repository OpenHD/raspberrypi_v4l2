// Definitions for CS-MIPI-SC132 camera module on VC I2C bus
/dts-v1/;
/plugin/;


/{
	compatible = "brcm,bcm2835";

	fragment@0 {
		target = <&i2c0if>;
		__overlay__ {
			status = "okay";
		};
	};

    fragment@1 {
			target-path="/";
			__overlay__ {

				veyecam_vdig: fixedregulator@1 {
					compatible = "regulator-fixed";
					regulator-name = "veyecam_vdig";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
				};
				veyecam_vddl: fixedregulator@2 {
					compatible = "regulator-fixed";
					regulator-name = "veyecam_vddl";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;
				};
                veyecam_vana: fixedregulator@3 {
					compatible = "regulator-fixed";
					regulator-name = "veyecam_vana";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};
				veyecam_clk: camera-clk {
					compatible = "fixed-clock";
					#clock-cells = <0>;
					clock-frequency = <24000000>;
				};
			};
		};

	fragment@2 {
		target = <&i2c0mux>;
		__overlay__ {
			status = "okay";
		};
	};
	i2c_frag: fragment@100 {
		target = <&i2c_csi_dsi>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			cssc132: cssc132@3b {
				compatible = "veye,cssc132";
				reg = <0x3b>;
				status = "okay";

                clocks = <&veyecam_clk>;
				clock-names = "xclk";

				VANA-supply = <&veyecam_vana>;	/* 2.8v */
				VDIG-supply = <&veyecam_vdig>;	/* 1.8v */
				VDDL-supply = <&veyecam_vddl>;	/* 1.2v */
                rotation = <0>;
				port {
					cssc132_0: endpoint {
						remote-endpoint = <&csi_ep>;
						clock-lanes = <0>;
						data-lanes = <1 2>;
						clock-noncontinuous;
						link-frequencies =
							/bits/ 64 <297000000>;
					};
				};
			};
		};
	};

	csi_frag: fragment@101 {
		target = <&csi1>;
		csi: __overlay__ {
			status = "okay";

			port {
				csi_ep: endpoint {
					remote-endpoint = <&cssc132_0>;
					clock-lanes = <0>;
					data-lanes = <1 2>;
					clock-noncontinuous;
				};
			};
		};
	};

	__overrides__ {
		rotation = <&cssc132>,"rotation:0";
		cam0 = <&i2c_frag>, "target:0=",<&i2c_vc>,
		       <&csi_frag>, "target:0=",<&csi0>;

	};


};
